      "source": [
        "class Solution(object):\n",
        "    def twoSum(self, nums, target):\n",
        "        n=nums\n",
        "        for i in range(len(n)):\n",
        "            for j in range(i+1,len(n)):\n",
        "                if (n[i]+n[j])==target:\n",
        "                    return [i,j]\n",
        "\n"
      ]
    }
  ]
}